{
    "luci-app-cloud-clipboard": {
        "description": "Grant access to cloud-clipboard settings",
        "read": {
            "file": {
                "/var/log/cloud-clipboard.log": [ "read" ]
            },
            "uci": [ "cloud-clipboard" ],
            "ubus": {
                "service": [ "list", "restart" ],
                "system": [ "board" ],
                "luci": [ "getProcessList" ]
            }
        },
        "write": {
            "file": {
                "/var/log/cloud-clipboard.log": [ "write" ]
            },
            "uci": [ "cloud-clipboard" ],
            "ubus": {
                "service": [ "list", "restart" ]
            }
        },
        "execute": {
            "file": {
                "/etc/init.d/cloud-clipboard": [ "exec" ]
            }
        }
    }
}